    "wrangler": "3.101.0",
    "@types/node": "22.7.7",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "autoprefixer": "10.0.1",
    "postcss": "8.4.35",
    "tailwindcss": "3.3.0",
    "typescript": "5.4.5"
  }
